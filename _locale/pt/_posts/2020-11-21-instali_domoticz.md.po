#, no-wrap
msgid ""
"sudo bash\n"
"adduser domotiko\n"
"cd /home/domotiko\n"
"curl -sSL install.domoticz.com | bash\n"
msgstr ""
"sudo bash\n"
"adduser domotiko\n"
"cd /home/domotiko\n"
"curl -sSL install.domoticz.com | bash\n"

